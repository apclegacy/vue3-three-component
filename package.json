{
  "name": "vue3-threejs",
  "version": "1.0.0",
  "description": "threejs component for vue3",
  "author": "alex peter charles (lexiboi)",
  "license": "MIT",
  "repository": "https://github.com/lexiboi/vue3-threejs",
  "main": "dist/three-sketch.ssr.js",
  "browser": "dist/three-sketch.esm.js",
  "module": "dist/three-sketch.esm.js",
  "unpkg": "dist/three-sketch.min.js",
  "types": "three-sketch.d.ts",
  "files": [
    "dist/*",
    "three-sketch.d.ts",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "three": "^0.124.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/preset-env": "^7.12.0",
    "@babel/preset-typescript": "^7.12.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "@vue/cli-plugin-babel": "^4.5.7",
    "@vue/cli-plugin-typescript": "^4.5.7",
    "@vue/cli-service": "^4.5.7",
    "@vue/compiler-sfc": "^3.0.0",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.4.1",
    "minimist": "^1.2.5",
    "rollup": "^2.30.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "typescript": "^3.8.3",
    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "engines": {
    "node": ">=10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
